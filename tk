import tkinter as tk

form = tk.Tk()
form.geometry("400x400+100+100")
form.title("Ders 4")
#state() komutu çıkan pencerenin şeklini tanımlar zoom-iconic-normal
#form.state("iconic")
form.state('normal')
#wm_attributes ta - alpha komutu pencerenin saydamlığını tayin eder.Sayı sıfıra ne kadar yakınsa pencere o kadar saydamlaşır
form.wm_attributes("-alpha",0.2)
form.mainloop()
